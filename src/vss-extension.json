{
    "manifestVersion": 1,
    "id": "epinova-dxp-deploy-extension",
    "publisher": "epinova-sweden",
    "version": "1.9.20",
    "name": "Epinova DXP deployment",
    "description": "Tasks to deploy NuGet package to Episerver DXP with DXP API. (Former: DXC)",
    "public": true,
    "icons":{
        "default": "img/Epinova_e_128x128.png"
    },
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "categories": [
        "Azure Pipelines"
    ],
    "tags":[
        "Release",
        "Epinova",
        "Episerver",
        "Deploy",
        "DXC",
        "DXP",
        "Publish",
        "Install",
        "Continuous Integration",
        "Continuous Delivery",
        "Build",
        "Release",
        "Azure Pipelines",
        "Episerver Content Cloud",
        "Episerver Commerce Cloud"
    ],
    "branding":{
        "color": "rgb(255,255,255)",
        "theme": "light"
    },
    "content": {
        "details": {
            "path": "overview.md"
        },
        "license": {
            "path": "license.md"
        }
    },
    "links": {
        "repository": {
            "uri": "https://github.com/Epinova/epinova-dxp-deployment"
        },
        "company": {
            "uri": "https://www.epinova.se"
        },
        "contact": {
            "uri": "mailto:info@epinova.se"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/Epinova/epinova-dxp-deployment"
    },
    "contributions": [
        {
            "id": "dxp-deploynugetpackage-buildtask",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "DeployNuGetPackage"
            }
        },
        {
            "id": "dxp-smoketestiffailreset-buildtask",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "SmokeTestIfFailReset"
            }
        },
        {
            "id": "dxp-completedeploy-buildtask",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "CompleteDeploy"
            }
        },
        {
            "id": "dxp-deployto-buildtask",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "DeployTo"
            }
        },
        {
            "id": "dxp-deploysmoothto-buildtask",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "DeploySmoothTo"
            }
        },
        {
            "id": "dxp-resetdeploy-buildtask",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "ResetDeploy"
            }
        },
        {
            "id": "dxp-exportdb-buildtask",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "ExportDb"
            }
        },
        {
            "id": "dxp-awaitstatus-buildtask",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "AwaitStatus"
            }
        },
        {
            "id": "dxp-expectstatus-buildtask",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "ExpectStatus"
            }
        },
        {
            "id": "dxp-contentcopy-buildtask",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "ContentCopy"
            }
        }
    ],
    "files": [
        {
            "path": "DeployNuGetPackage"
        },
        {
            "path": "SmokeTestIfFailReset"
        },
        {
            "path": "CompleteDeploy"
        },
        {
            "path": "DeployTo"
        },
        {
            "path": "DeploySmoothTo"
        },
        {
            "path": "ResetDeploy"
        },
        {
            "path": "ExportDb"
        },
        {
            "path": "AwaitStatus"
        },
        {
            "path": "ExpectStatus"
        },
        {
            "path": "ContentCopy"
        },
        {
            "path": "node_modules/vss-web-extension-sdk/lib",
            "addressable": true,
            "packagePath": "lib"
        }
    ]
}